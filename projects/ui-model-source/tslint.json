{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "only-arrow-functions": false,
    "typedef": false,
    "quotemark": false,
    "variable-name": false,
    "triple-equals": false,
    "no-empty-interface": false,
    "max-line-length": false,
    "no-trailing-whitespace": false
  }
}
